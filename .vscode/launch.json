  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Build Content",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**/*.js",
        "lib/**/*.js",
        "async_hooks.js",
        "inspector_async_hook.js"
      ],
      "program": "${workspaceFolder}/build/buildJobsContent.js"
    }
  ]
}
