{
  "extends": ["airbnb", "plugin:prettier/recommended", "plugin:react/recommended", "plugin:jsx-a11y/recommended"],
  "plugins": ["prettier", "react"],
  "env": {
    "node": true,
    "browser": true
  },

  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0],
    "react/no-danger": [0],
    "import/no-named-as-default": [0],
    "import/no-named-as-default-member": [0]
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
